    <!-- Gallery -->
<div class="col_6 gallery">
	<article class="box weather float_left">

	  <div class="icon bubble black">
	    <div class="spin">
	      <img src="/images/glucometro.png">
	    </div>
	  </div>
	  
	  <h1>Glucosa</h1>
	  <input class="myButton" ng-click="button_leer_glucometro()" type="button" value="Analizar Glucosa En Sangre">
	  <h2>{{glucometer_read_value}} mg/dl</h2>

	  <div class="icon bubble black">
	    <div class="spin">
	      <img src="/images/presion.png">
	    </div>
	  </div>
	  
	  <h1>Presión Arterial</h1>
	  <input class="myButton" ng-click="button_leer_glucometro()" type="button" value="Analizar Presión Arterial">
	  <h2>{{blood_pressure_read_value}} p</h2>
	  <h2>{{systolic_pressure_read_value}} SYS</h2>
	  <h2>{{diastolic_pressure_read_value}} DIA</h2>

	</article>
</div>


	<article class="box weather float_left">

	  <div class="icon bubble black">
	    <div class="spin">
	      <img src="/images/oximetro.png">
	    </div>
	  </div>
	  
	  <h1>Oximetría de Pulso</h1>
	  <input class="myButton" ng-click="button_leer_oximetro()" type="button" value="Medición de Oxígeno Arterial">
	  <h2>{{oximeter_read_value_spo2}} SpO2%</h2>
	  <h2>{{oximeter_read_value_prbpm}} PRbpm</h2>
	</article>


	<article class="box weather float_left">

	  <div class="icon bubble black">
	    <div class="spin">
	      <img src="/images/temperatura.png">
	    </div>
	  </div>
	  
	  <h1>Temperatura</h1>
	  <input class="myButton" ng-click="button_leer_temperatura()" type="button" value="Medición de Temperatura Corporal">
	  <h2>{{temperature_read_value}} &#8451;</h2>
	</article>
